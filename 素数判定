import java.util.*;
 
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		System.out.println(hantei(a));
	}
	public static boolean hantei(int a){
		for(int i = 2; i * i <= a; i++){
			if(a%i == 0) return false;
		}
		return true;
	}
}
